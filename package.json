    "prod": "MONGODB_URI=mongodb://heroku_9q2wscb0:em70tededsbvhtuenn0tugo61t@ds241121.mlab.com:41121/heroku_9q2wscb0 nodemon --exec 'ts-node --files' ./src/index.ts",
    "start-mongo": "docker run -d -p 27017:27017 -v ~/Work/Projects/agendar-cc/agendar-cc-backend/data/mongo:/data/db mongo",
    "start": "NODE_ENV=production node dist/index.js",
    "start-worker": "NODE_ENV=production node dist/worker/index.js",
    "start-worker:dev": "ts-node ./src/worker/index.ts",
    "start:dev": "node dist/index.js",
