{
  "name": "agendar-cc",
  "version": "0.1.0",
  "description": "An App to help Portuguese citizens to schedule an appointment to get their Citizen Card",
  "main": "dist/index.js",
  "scripts": {
    "copy-public": "cp -r ./src/app/public dist/app/public",
    "prebuild": "npm run clean && npm run lint",
    "postbuild": "npm test && npm run copy-public",
    "build": "npm run tsc",
    "tsn": "ts-node",
    "tsc": "tsc --project tsconfig.dist.json",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json",
    "worker:dev": "nodemon --exec 'ts-node --files' ./src/worker/index.ts",
    "dev": "nodemon --exec 'ts-node --files' ./src/index.ts",
    "prod": "MONGODB_URI=mongodb://heroku_9q2wscb0:em70tededsbvhtuenn0tugo61t@ds241121.mlab.com:41121/heroku_9q2wscb0 nodemon --exec 'ts-node --files' ./src/index.ts",
    "start-mongo": "docker run -d -p 27017:27017 -v ~/Work/Projects/agendar-cc/agendar-cc-backend/data/mongo:/data/db mongo",
    "start": "NODE_ENV=production node dist/index.js",
    "start-worker": "NODE_ENV=production node dist/worker/index.js",
    "start-worker:dev": "ts-node ./src/worker/index.ts",
    "start:dev": "node dist/index.js",
    "test": "jest --testPathPattern=test/.+\\.spec\\.ts",
    "test:watch": "npm test -- --watch",
    "prettier": "prettier --no-semi --tab-width 2 --print-width 120 --trailing-comma all --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "prettier-fix": "prettier --no-semi --tab-width 2 --print-width 120 --trailing-comma all --parser typescript --write \"{src,test}/**/*.ts\""
