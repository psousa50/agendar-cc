{
  "name": "agendar-cc",
  "version": "0.1.0",
  "description": "An App to help Portuguese citizens to schedule an appointment to get their Citizen Card",
  "main": "src/server/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build:client:dev": "tsc --project client/config/tsconfig.json && webpack --config=client/config/webpack.config.dev.js",
    "build:server:dev": "tsc --project server/config/tsconfig.json",
    "build:dev": "npm run build:server:dev",
    "build:client": "tsc --project client/config/tsconfig.json && NODE_ENV=production webpack --config=client/config/webpack.config.production.js",
    "clean": "rm -rf cache dist tmp ts-node",
    "dev": "nodemon --exec 'ts-node --files --project server/config/tsconfig.json' server/src/index.ts",
    "lint": "tslint --project tsconfig.json",
    "start": "node dist/src/server/index.js",
    "test": "jest --testPathPattern=test/.+\\.spec\\.tsx?",
    "test:watch": "npm test -- --watch",
    "prettier": "prettier --no-semi --tab-width 2 --print-width 120 --trailing-comma all --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "prettier-fix": "prettier --no-semi --tab-width 2 --print-width 120 --trailing-comma all --parser typescript --write \"{src,test}/**/*.ts\""
  },
  "author": "psousa",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "convict": "^5.1.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^3.2.0",
    "css-modules-require-hook": "^4.2.3",
    "csurf": "^1.9.0",
    "express": "^4.17.1",
    "fp-ts": "^2.0.5",
    "io-ts": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "node": "^12.9.0",
    "prettier": "^1.18.2",
    "ramda": "^0.26.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.0.1",
    "react-select": "^3.0.4",
    "reactstrap": "^8.0.1",
    "response-time": "^2.3.2",
    "serialize-javascript": "^1.8.0",
    "style-loader": "^1.0.0",
    "styled-components": "^4.3.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/convict": "4.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.35",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/express": "^4.17.1",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^10.14.16",
    "@types/ramda": "^0.26.19",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.9",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-select": "^3.0.2",
    "@types/reactstrap": "^8.0.4",
    "@types/response-time": "^2.3.4",
    "@types/serialize-javascript": "^1.3.2",
    "@types/uuid": "^3.4.5",
    "assets-webpack-plugin": "^3.9.6",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jsdom": "^15.1.1",
    "jsverify": "^0.8.3",
    "nodemon": "^1.19.1",
    "ts-jest": "<24.1.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "typescript-tslint-plugin": "^0.5.4",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
